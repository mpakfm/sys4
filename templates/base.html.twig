<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <link rel="icon" href="/build/images/favicon.ico">
        <title>{% block title %}{{ meta.title }}{% endblock %}</title>
        <meta name="keywords" content="{% block keywords %}{{ meta.keywords }}{% endblock %}" />
        <meta name="description" content="{% block description %}{{ meta.description }}{% endblock %}" />
        <!--<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">-->
        <link rel="canonical" href="{{ canonical }}"/>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}

        <!--   Core JS Files   -->
        <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
        <script src="/build/admin/js/counter.js"></script>
{#        <script src="/build/admin/js/core/popper.min.js"></script>#}
{#        <script src="/build/admin/js/core/bootstrap.min.js"></script>#}
    </body>
</html>
